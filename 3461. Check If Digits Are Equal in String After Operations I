class Solution {
    public boolean hasSameDigits(String s) {
        int n =s.length();
        int[] numbers = new int[s.length()];
        for (int i = 0; i < n; i++) {
            numbers[i] = s.charAt(i) -'0'; //string-> arr
        }
        while(n>2){
            for(int i=0;i<n-1;i++){
                numbers[i]=(numbers[i]+numbers[i+1])%10;
            }
            n --;
        }
return numbers[0]==numbers[1];
    }
}
